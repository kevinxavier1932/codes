public class reverse {
    public static void main(String[] args) {
    int arr[]={5,6,9,8,9};
    int a2[]=new int [arr.length];
    int c=0;
    for(int i=arr.length-1;i>=0;i--){
        a2[c]=arr[i];
        c++;
    }
    for(int k=0;k<a2.length;k++){
      System.out.println(a2[k]);
    }
    }
}
