public class sumofarray {
    public static void main(String[] args) {
    int [][]a={{1,2,3,4},{5,2,3,6},{2,6,5,4},{9,5,3,5}};
    int sum=0;
    for(int i=0;i<a.length;i++){
        for(int j=0;j<a.length;j++){
            sum=sum+a[i][j];
            
        }
    }
    System.out.println(sum);
    }
}
