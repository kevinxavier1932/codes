import java.util.Scanner;
class main{
    public static void main(String[]args){
    Scanner scan=new Scanner(System.in);
    System.out.println("Enter the number");
    int a=scan.nextInt();
    int sum=0;
    while(a>0)
    {
        int dig=a%10;
        sum=sum+dig;
        a=a/10;
    }
   System.out.println(sum);
    }
    
}
